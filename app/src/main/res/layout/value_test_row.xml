<?xml version="1.0" encoding="utf-8"?>
<com.google.android.material.card.MaterialCardView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/cardViewRootView"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="18dp"
    android:layout_marginBottom="18dp"
    android:layout_marginStart="18dp"
    android:layout_marginEnd="18dp"
    style="@style/ThemeOverlay.DanaTest031.TestMaterialCardViewElevation">

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/quest_pair_value"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/dt3_brand_100"
        android:paddingStart="18dp"
        android:paddingTop="14dp"
        android:paddingEnd="14dp"
        android:paddingBottom="14dp"
        android:textAppearance="@style/dt_QuestCardTextStyle"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />

</com.google.android.material.card.MaterialCardView>