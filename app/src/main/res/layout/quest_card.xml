<?xml version="1.0" encoding="utf-8"?>
<com.google.android.material.card.MaterialCardView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/cardViewRootView"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="18dp"
    android:layout_marginStart="18dp"
    android:layout_marginEnd="18dp"
    android:minHeight="132dp"
    app:cardElevation="0dp"
    android:background="@drawable/quest_card_with_stoke_background_drawable"
    android:backgroundTintMode="src_over"
    >

    <!--style="@style/ThemeOverlay.DanaTest031.TestMaterialCardViewElevation"-->

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/quest_pair_value"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:gravity="center"
        android:layout_gravity="center"
        style="@style/TextAppearance.dt_pairsetItemTitle"
        android:textSize="46sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"

        />

    <!--android:textAppearance="@style/dt_QuestCardTextStyle"-->

</com.google.android.material.card.MaterialCardView>