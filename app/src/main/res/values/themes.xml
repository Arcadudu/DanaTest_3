<resources>
    <!-- Base application theme. -->




    <style name="Theme.DanaTest031" parent="Theme.MaterialComponents.Light.NoActionBar">

        <!--colors-->
        <item name="colorPrimary">@color/dt3_main_light_100</item>
        <item name="colorPrimaryVariant">@color/dt3_main_light_70</item>
        <item name="colorPrimaryDark">@color/dt3_main_light_100</item>
        <item name="colorOnPrimary">@color/dt3_on_main</item>
        <item name="colorSecondary">@color/dt3_brand_100</item>
        <item name="colorSecondaryVariant">@color/dt3_brand_70</item>
        <item name="colorOnSecondary">@color/dt3_on_brand_100</item>
        <item name="android:colorBackground">@color/dt3_main_light_100</item>
        <item name="colorSurface">@color/dt3_surface_100</item>
        <item name="colorError">@color/dt3_error_100</item>
        <item name="colorOnError">@color/dt_on_error_100</item>
        <item name="colorControlHighlight">@color/dt3_highlight_20</item>
        <item name="android:windowLightStatusBar">true</item>

        <!--components-->
        <item name="materialCardViewStyle">@style/dt_CardViewStyle_listItem</item>
        <item name="editTextStyle">@style/dt_EditTextStyle_searchField</item>
        <item name="floatingActionButtonStyle">@style/dt_FloatingActionButtonStyle</item>
        <item name="textInputStyle">@style/dt_TextInputLayoutStyle</item>

    </style>

    <!--DT overlays-->
    <style name="ThemeOverlay.DanaTest031.BrandOpaqueControlHighlight" parent="">
        <item name="colorControlHighlight">@color/dt3_highlight_20</item>
    </style>

    <style name="ThemeOverlay.DanaTest031.VioletPrimary" parent="">
        <item name="colorPrimary">@color/dt3_hint_color_30</item>
    </style>

    <style name="ThemeOverlay.DanaTest031.TestMaterialCardViewElevation" parent="">
        <item name="cardElevation">3.4dp</item>
    </style>

    <style name="AlertDialogTheme" parent="Theme.AppCompat.DayNight.Dialog.Alert">
        <item name="buttonBarPositiveButtonStyle">@color/black</item>
    </style>

</resources>